CONFIGURE_FLAGS=" \
  CXX=mpic++ \
  CC=mpicc \
  F77=gfortran \
  CXXFLAGS='-fPIC -DNDEBUG -std=c++11 -O3 -DHAVE_PARMETIS=0 -g -w -DDUNE_STUFF_FUNCTIONS_DISABLE_CHECKS' \
  --enable-parallel \
  --without-parmetis \
  --disable-documentation \
  INKSCAPE=no"
CMAKE_FLAGS="-Wno-dev -DCMAKE_BUILD_TYPE='' -DSTDTHREAD_LINK_FLAGS='-lpthread -pthread'"
BUILDDIR=stinson_gcc_release
