CONFIGURE_FLAGS="CXX=g++ CC=gcc FC=gfortran \
  CXXFLAGS='-DNDEBUG -std=c++11  -g -w -DDUNE_STUFF_FUNCTIONS_DISABLE_CHECKS -funroll-loops -m64 -mfpmath=sse -falign-loops -mtune=native -march=native -pipe -fomit-frame-pointer -O4' \
  --enable-parallel \
  --disable-documentation  INKSCAPE=no"
CMAKE_FLAGS="-DGRIDDIM=2 -Wno-dev -DCMAKE_BUILD_TYPE='' "\
MAKE_FLAGS="-- -j1 -l1"
BUILDDIR=$HOME/multiscale-build-gcc
