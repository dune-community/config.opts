DIR="$(cd "$(dirname ${BASH_SOURCE[0]})" ; pwd -P )"
NAME="$(basename ${BASH_SOURCE[0]})"
source ${DIR}/palma_common
CONFIGURE_FLAGS="
  CC=icc \
  CXX=icc \
  CXXFLAGS='-DDEBUG -g -O3 -std=c++0x -DDUNE_STUFF_FUNCTIONS_DISABLE_CHECKS -fno-omit-frame-pointer -fsanitize=thread' \
  --disable-documentation \
  --enable-parallel \
  INKSCAPE=no \
"

